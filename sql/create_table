create table if not exists my_table (
id integer primary key autoincrement,
Run text,
Filtered text default 'No',
lane integer,
mid integer,
number_of_groups_analyzed integer default 0,
number_of_raw_groups integer default 0,
number_of_contaminations integer default 0,
a_to_c integer default 0,
a_to_g integer default 0,
a_to_t integer default 0,
c_to_a integer default 0,
c_to_g integer default 0,
c_to_t integer default 0,
g_to_a integer default 0,
g_to_c integer default 0,
g_to_t integer default 0,
t_to_a integer default 0,
t_to_c integer default 0,
t_to_g integer default 0,
hetero_a_to_c integer default 0,
hetero_a_to_g integer default 0,
hetero_a_to_t integer default 0,
hetero_c_to_a integer default 0,
hetero_c_to_g integer default 0,
hetero_c_to_t integer default 0,
hetero_g_to_a integer default 0,
hetero_g_to_c integer default 0,
hetero_g_to_t integer default 0, 
hetero_t_to_a integer default 0,
hetero_t_to_c integer default 0,
hetero_t_to_g integer default 0,
a_to__ integer default 0,
c_to__ integer default 0,
g_to__ integer default 0,
t_to__ integer default 0, 
__to_a integer default 0,
__to_c integer default 0,
__to_g integer default 0,
__to_t integer default 0,
other_ integer default 0,
hetero_a_to__ integer default 0,
hetero_c_to__ integer default 0, 
hetero_g_to__ integer default 0,
hetero_t_to__ integer default 0,
hetero___to_a integer default 0,
hetero___to_c integer default 0,
hetero___to_g integer default 0,
hetero___to_t integer default 0,
hetero_other_ integer default 0);
