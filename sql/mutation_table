create table if not exists mutation_table (
[id] integer primary key autoincrement,
[Run] text,
[data processed/analyzed] text,
[Filtered] text,
[lane] text,
[mid] text,
[number of groups analyzed] integer default 0,
[number of raw groups] integer default 0,
[number of contaminations] integer default 0,

[g to :] integer default 0,
[g to a] integer default 0,
[g to c] integer default 0,
[g to t] integer default 0,
[t to :] integer default 0, 
[t to a] integer default 0,
[t to c] integer default 0,
[t to g] integer default 0,
[c to :] integer default 0,
[c to a] integer default 0,
[c to t] integer default 0,
[c to g] integer default 0,
[a to :] integer default 0,
[a to t] integer default 0,
[a to c] integer default 0,
[a to g] integer default 0,
[: to t] integer default 0,
[: to a] integer default 0,
[: to c] integer default 0,
[: to g] integer default 0,
[other] integer default 0,

[hetero g to :] integer default 0,
[hetero g to a] integer default 0,
[hetero g to c] integer default 0,
[hetero g to t] integer default 0,
[hetero t to :] integer default 0, 
[hetero t to a] integer default 0,
[hetero t to c] integer default 0,
[hetero t to g] integer default 0,
[hetero c to :] integer default 0,
[hetero c to a] integer default 0,
[hetero c to t] integer default 0,
[hetero c to g] integer default 0,
[hetero a to :] integer default 0,
[hetero a to t] integer default 0,
[hetero a to c] integer default 0,
[hetero a to g] integer default 0,
[hetero : to t] integer default 0,
[hetero : to a] integer default 0,
[hetero : to c] integer default 0,
[hetero : to g] integer default 0,
[hetero other] integer default 0

);
